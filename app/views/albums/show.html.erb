  <h1 class="text-center"><%=@album.name%></h1>
   <%= link_to 'Edit', edit_album_path(@album) %> |
<%= link_to 'Back', albums_path %>
 <span><%= link_to "Eliminar",@album,method: :delete, data: {confirm: "Estas seguro?"}%> </span>

  <div class="gallery">
    <%if @photos.empty?%>
    <h2 class="text-center">No hay fotos para mostrar</h2>
    <%else%>
      <%@photos.each do |photo|%>
            <div class="thumb">
          <%=image_tag photo.image_url() if photo.image_url%>
          </div>
    <%end%>
    <%end%>
 </div>
